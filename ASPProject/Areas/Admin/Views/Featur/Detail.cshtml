@model FeaturDetailVM




<div class="container">
    <div class="form-group">
        <h2><label class="col-md-12" style="font-weight:700">Featur Detail</label></h2>
        <div class="col-md-12 mt-3">
            <h3>Title:<input asp-for="Title" type="text" disabled class="form-control form-control-line" /></h3>
            <h3>Description:<input asp-for="Description" type="text" disabled class="form-control form-control-line" /></h3>
            <h3>Icon:</h3><i class="@Model.Icon"></i>
        </div>
    </div>

    <div class="form-group mt-3">
        <div class="col-sm-12">
            <a asp-action="Index" class="btn btn-primary">Back</a>
        </div>
    </div>
</div>

